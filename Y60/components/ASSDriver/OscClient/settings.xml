<settings>
    <ASSDriver>
        <GainPower>2</GainPower>
        <ComponentThreshold>10.00000</ComponentThreshold>
        <NoiseThreshold>15</NoiseThreshold>
        <IntensityThreshold step="0.5">9.00000</IntensityThreshold>
        <FirstDerivativeThreshold step="1">30.00000</FirstDerivativeThreshold>
        <MinTouchInterval step="0.01">0.2</MinTouchInterval>
        <ClampToScreen step="1">0</ClampToScreen>

        <ProbePosition min="-1" step="1">[2.00000,3.00000]</ProbePosition>
        <DebugTouchEvents min="0" max="1" step="1">0</DebugTouchEvents>
        <CurrentValueDisplay min="0" max="2">0.00000</CurrentValueDisplay>

        <SerialPort>4</SerialPort>
        <UseUSB>0</UseUSB>
        <BaudRate>57600</BaudRate>
        <BitsPerWord>8</BitsPerWord>
        <Parity>no_parity</Parity>
        <StopBits>1</StopBits>
        <Handshaking>false</Handshaking>

        <ServerAddress>127.0.0.1</ServerAddress>
        <ServerPort>12000</ServerPort>
        <ClientPort>7001</ClientPort>
        <TouchThreshold step="5">200</TouchThreshold>
    </ASSDriver>
</settings>
